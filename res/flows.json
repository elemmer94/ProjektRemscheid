                "c573e5396d251d09"
            ]
        ]
    },
    {
        "id": "c573e5396d251d09",
        "type": "function",
        "z": "caee75c53553d1b6",
        "name": "Berechne Mittelwert",
        "func": "let values = flow.get(\"values\") || [];\n\nvalues.push(msg.payload);\n\nlet sum = values.reduce((a, b) => a + b, 0);\nlet avg = sum / values.length;\n\n// Speicher zur√ºcksetzen\nflow.set(\"values\", []);\n\nmsg.payload = avg;\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 440,
        "y": 40,
        "wires": [
            []
        ]
